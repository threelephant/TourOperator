@{
    ViewData["Title"] = "Добавление файла";
}

<h2>Выберите файл для загрузки</h2>
<form asp-action="file", asp-controller="admin" method="post" enctype="multipart/form-data">
<div class="file-field input-field">
      <div class="btn">
        <span>File</span>
        <input type="file" name="uploadedFile">
      </div>
      <div class="file-path-wrapper">
        <input class="file-path validate" type="text">
      </div>
    </div>
    <button class="btn waves-effect waves-light" type="submit" name="action">Загрузить файл</button>
</form>